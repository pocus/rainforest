<h1>Products#index</h1>

<%= link_to "new", new_product_path %>

<ul>

	<% @products.each do |p| %>
	<p>
		<li><%= link_to p.name, product_path(p) %></li>
		<li><%= p.description %></li>
		<li>$0.<%= p.price_in_cents %></li>
		<li><%= link_to "edit", edit_product_path(p) %>
	</p>
	<% end %>

</ul>


